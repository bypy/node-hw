					<xsd:documentation>Документ, удостоверяющий личность, имеет признаки подделки</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="paspForgeryDetails" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Признаки подделки</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="paspDisruptionFlag" type="tns:booleanFlag">
				<xsd:annotation>
					<xsd:documentation>Документ, удостоверяющий личность, имеет повреждения/нарушения структуры</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="paspDisruptionDetails" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Признаки повреждения/нарушения структуры</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="finDocForgeryFlag" type="tns:booleanFlag">
				<xsd:annotation>
					<xsd:documentation>Документ, подтверждающий финансовое состояние клиента, имеет признаки подделки</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="finDocForgeryDetails" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Признаки подделки финансового документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="paspSignatureMismatchFlag" type="tns:booleanFlag">
				<xsd:annotation>
					<xsd:documentation>Подпись клиента в документе, удостоверяющем личность, не соответствует подписи в Анкете</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="paspSignatureMismatchDetails" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Комментарии к пункту Подпись клиента в документе, удостоверяющем личность, не соответствует подписи в Анкете</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
